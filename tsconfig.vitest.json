	"exclude": [],
	"compilerOptions": {
		"composite": true,
        "noImplicitAny": false,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
		"lib": [],
		"types": ["node", "jsdom"],
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
